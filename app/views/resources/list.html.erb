<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Assets</h1>
    </div>
    <% for resource in @resources %>
      <div class="col-lg-3 col-md-4 col-xs-6 thumb">
        <a class="thumbnail" href="<%=resource.id %>">
            <%= image_tag resource.url.to_s, class: "img-responsive" %>
        </a>
      </div>
    <% end %>
</div>
